// Arrow boi (Back and forth movement)
if arrowTimerMode == 0
{
    if arrowTimer &gt; 4
    {
        arrowTimerMode = 1 //minus
    }
    arrowTimer += 0.5
}
else if arrowTimerMode == 1
{
    if arrowTimer &lt; 0
    {
        arrowTimerMode = 0
    }
    arrowTimer -= 0.5
}

