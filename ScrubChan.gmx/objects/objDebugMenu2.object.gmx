// Recognize input
if kDown
{
if menuChoice &lt; 9 { menuChoice += 1 }
}

if kUp
{
if menuChoice &gt; 0 { menuChoice -= 1 }
}

// Update flavor text
if menuChoice == 1 { flavorText = "START A NEW GAME!" }
else if menuChoice == 2 { flavorText = "LOAD A PREVIOUS SAVE!" }
else if menuChoice == 3 { flavorText = "MODIFY GAME SETTINGS!" }
else if menuChoice == 4 { flavorText = "EXIT TO OS!" }
else if menuChoice == 5 { flavorText = "GUIDING KEY" }
else { flavorText = string(menuChoice) + " out of bounds"; }

// Arrow Y
if menuChoice == 1 { arrowY = 48 }
else if menuChoice == 2 { arrowY = 64 }
else if menuChoice == 3 { arrowY = 80 }
else if menuChoice == 4 { arrowY = 96 }
else if menuChoice == 5 { arrowY = 112 }
// Confirm
if kConfirm
{
    if menuChoice == 1 || menuChoice == 2 || menuChoice == 3 || menuChoice == 4 || menuChoice == 5 || menuChoice == 6 || menuChoice == 7 || menuChoice == 8 || menuChoice == 9
    {
        room_goto(rmDoesNotExist)
    }
}

if keyboard_check_released(ord("T"))
{
    instance_create(0,0,objDebugMenu)
    instance_destroy();
}
// Sound

if kUp || kDown
{
    audio_play_sound(sndMenuNavigate, 9, false)
}
