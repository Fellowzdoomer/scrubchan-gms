draw_sprite(sprMenuArrow,0,view_xview[0] + arrowX, view_yview[0] + arrowY);

draw_text(view_xview[0]+128, view_yview[0] + 64, "SELECT DIFFICULTY");

draw_text(view_xview[0]+128, view_yview[0] + 92, "BEGINNER");
draw_text(view_xview[0]+128, view_yview[0] + 104, "STANDARD");
draw_text(view_xview[0]+128, view_yview[0] + 116, "PROUD");
draw_text(view_xview[0]+128, view_yview[0] + 128, "CRITICAL");


draw_sprite(sprDifficultyBox, 0, view_xview[0], view_yview[0] + 176);
draw_text(view_xview[0]+128, view_yview[0] + 200, flavorText);
</string>
          </argument>
