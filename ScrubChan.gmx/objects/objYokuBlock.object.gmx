            <kind>1</kind>
            <string>if (!_continue)
{
    switch appearance
    {
        case "dev": sprite_index = sprDevYokuBlock; break;
    }

    _continue = true;
}

if (_continue) &amp;&amp; (isActive)
{
    var timerMax;
    
    switch cycle
    {
        case 1: timerMax = 180; break;
        case 2: timerMax = 120; break;
        case 3: timerMax = 60; break;
        case 4: timerMax = 1; break;
    }
    /*//if (cycle == 1) timerMax = 60*cycle;
    if (cycle == cycleMax) timerMax = 60;
    else timerMax = ((60*cycleMax) - (60*cycle))
    //if timerMax == 0 timerMax = 60;*/
    // sprite handling first
    {
        if (image_index == 4) image_speed = 0;
    }
    
    if visible { if place_meeting(x,y,objPlayer) with objPlayer y -= 16; }  // move player up if spawned inside
    
    switch phase
    {
        
        case 0: // initial
            timer += 1;
            if (timer == 60 * cycle)
            {
                instance_create(x,y,objSolid); // place down a solid object
                audio_play_sound(sndYokuBlock, 1, false);
                visible = true;
                image_index = 0;
                image_speed = 10/60;
                timer = 0;
                phase = 1;
            }
        break;
        
        case 1: // active
            timer += 1;
            if (timer == 120)
            {
                var solidHitbox = instance_nearest(x,y,objSolid);
                with solidHitbox instance_destroy();
                visible = false;
                phase = 2;
                timer = 0;
            }
        break;
        
        case 2: // wait for respawn
            timer += 1;
            if (timer == timerMax)
            {
                timer = 0;
                phase = 0;
            }
        break;
    }
}
