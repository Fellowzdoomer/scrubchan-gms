else draw_sprite_ext(sprMetalSimulation,3,room_width/2-64,room_height/2,1,1,0,c_white,0.5);

draw_sprite_ext(sprMenuArrow2,0,16,112,2,2,0,c_white,1);
draw_sprite_ext(sprMenuArrow,0,240,112,2,2,0,c_white,1);

