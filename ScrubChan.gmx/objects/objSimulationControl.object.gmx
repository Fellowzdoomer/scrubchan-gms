draw_set_color(c_white);
draw_set_halign(fa_center);

draw_sprite_ext(sprMetalSimulation,menuChoice,room_width/2,room_height/2,2,2,0,c_white,1);
if menuChoice+1 &lt;= 3 draw_sprite_ext(sprMetalSimulation,menuChoice+1,room_width/2+64,room_height/2,1,1,0,c_white,0.5);
else draw_sprite_ext(sprMetalSimulation,0,room_width/2+64,room_height/2,1,1,0,c_white,0.5);

