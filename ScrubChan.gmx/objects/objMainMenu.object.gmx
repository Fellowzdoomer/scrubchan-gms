            <string>menuChoice = 1;
flavorText = "A CHOICE HAS YET TO BE MADE!"

arrowTimer = 0;
arrowTimerMode = 0; // 0 - plus, 1 - minus
arrowY = 16;

menuArea = "main";

screenres = "2X";
