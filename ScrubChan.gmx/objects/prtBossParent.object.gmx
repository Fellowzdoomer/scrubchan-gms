        if enemyTimer2 &gt;= hitTimer
        {
            enemyTimer2 = 0;
            isHit = false;
        }
    }

    if invulnerable == true
    {
    invulnTimer += 1;
    
    if invulnTimer % 2 == 0 {visible = false; } else {visible = true; }
    if invulnTimer == 128
    {
        visible = true;
        invulnerable = false;
        invulnTimer = 0;
    }
}
